algoritmo "semnome"
// Função :
// Autor :
// Data : 13/08/2022
// Seção de Declarações 
var
time:caracter
inicio
escreva("informe o time")
leia(time)
escolha(time)
   caso "flamengo","fluminense","vasco"
     escreva("time do RJ")
   caso "santos","palmeiras","corinthians"
     escreva("time de SP")
   outrocaso
     escreva("time de outro estado")

fimescolha
// Seção de Comandos 
fimalgoritmo
