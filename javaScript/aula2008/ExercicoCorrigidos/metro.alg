algoritmo "semnome"
// Função :
// Autor :
// Data : 20/08/2022
// Seção de Declarações
var
ana, fel:real
ano:inteiro
inicio
// Seção de Comandos
ana := 1.50        //2 ano
fel := 1.10        // 3 ano
ano := 0

enquanto (ana > fel) faca
ana:= ana +0.02
fel:= fel + 0.03
ano:= ano+ 1
fimenquanto

fimalgoritmo


