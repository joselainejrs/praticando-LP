algoritmo "semnome"
// Função :
// Autor :
// Data : 27/08/2022
// Seção de Declarações
var
fatorial, cont, resultado: inteiro
inicio
// Seção de Comandos
escreval("informe o número")
leia(fatorial)
resultado := fatorial
cont := fatorial - 1
enquanto (cont > 1) faca
    resultado := resultado * cont
    cont := cont - 1
fimenquanto
escreva("o resultado é", resultado)
fimalgoritmo
